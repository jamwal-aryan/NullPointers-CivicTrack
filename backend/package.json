{
  "name": "civic-track-backend",
  "version": "1.0.0",
  "description": "Backend API for CivicTrack - A citizen engagement platform for reporting and tracking civic issues",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:setup-help": "node scripts/setup-database.js",
    "db:init-sqlite": "node scripts/init-sqlite.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/seed.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:validate": "node scripts/validate-models.js"
  },
  "keywords": [
    "civic",
    "issues",
    "reporting",
    "geolocation",
    "community"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "sequelize": "^6.35.0",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
